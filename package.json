{
  "name": "swagger-angular-generator",
  "version": "1.0.13",
  "description": "Generator of API layer in TypeScript for Angular 2+ apps",
  "main": "dist/generate.js",
  "types": "dist/generate.d.ts",
  "bin": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "build": "tsc --outDir dist",
    "prebuild": "rm -rf dist",
    "test-generate": "ts-node ./tests/test-init",
    "eee": "karma start --single-run",
    "test": "npm run test-generate && karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jnwltr/swagger-angular-generator.git"
  },
  "keywords": [
    "swagger",
    "api",
    "client",
    "typescript",
    "generate",
    "generator",
    "angular"
  ],
  "author": "Jan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnwltr/swagger-angular-generator/issues"
  },
  "homepage": "https://github.com/jnwltr/swagger-angular-generator#readme",
  "dependencies": {
    "commander": "^2.11.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-server-side": "^1.7.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@angular/common": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@types/commander": "^2.11.0",
    "@types/jasmine": "^2.5.35",
    "@types/lodash": "^4.14.77",
    "@types/node": "^8.0.47",
    "angular-2-local-storage": "^1.0.1",
    "chrome-remote-interface": "^0.25.2",
    "fs-extra": "^4.0.2",
    "gulp": "^3.9.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.5.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-typescript": "^3.0.8",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.18"
  },
  "peerDependencies": {
    "@angular/common": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "angular-2-local-storage": "^1.0.1",
    "zone.js": "^0.8.18",
    "rxjs": "^5.5.0"
  }
}
